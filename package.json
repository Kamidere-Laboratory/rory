{
  "name": "rory-core",
  "version": "0.0.1",
  "description": "Rory - Modular discord bot core based on eris",
  "main": "compiled/index.js",
  "scripts": {
    "build:container": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose build --parallel builder",
    "watch:bot:container": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose run --rm builder",
    "start:bot:container": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose run --rm rory"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/kamidere-laboratory/rory.git"
  },
  "keywords": [
    "rory",
    "discord",
    "eris",
    "modular",
    "bot"
  ],
  "author": {
    "name": "Kamide.re Laboratory",
    "url": "https://gitlab.com/kamidere-laboratory"
  },
  "contributors": [
    {
      "name": "Robert \"RunAge\" Gajda",
      "url": "https://gitlab.com/RunAge"
    }
  ],
